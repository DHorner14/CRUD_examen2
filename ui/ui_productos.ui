  <property name="styleSheet">
   <string notr="true">/* === Estilo general (Dark Mode) === */
QMainWindow {
    /* Fondo principal oscuro */
    background-color: #1e1e1e; 
    /* Texto principal claro */
    color: #cccccc;
    font-family: &quot;Segoe UI Variable&quot;, &quot;Roboto&quot;, &quot;Open Sans&quot;, sans-serif;
    font-size: 16px;
    font-weight: 300; 
}

/* === Frames === */
QFrame {
    /* Fondo de frames, más oscuro que el fondo principal */
    background-color: #252526; 
    border-radius: 10px;
    /* Borde sutil */
    border: 1px solid #444444; 
}

/* Barra superior */
#frame_superior {
    /* Fondo para la barra superior, ligeramente diferente */
    background-color: #2d2d30; 
    border: none;
}

/* Panel lateral */
#frame_lateral {
    /* Fondo del panel lateral */
    background-color: #2d2d30; 
    /* Borde derecho sutil */
    border-right: 1px solid #444444; 
}

/* Área principal */
#frame_paginas {
    /* Fondo del área principal */
    background-color: #1e1e1e;
}

/* --- */

/* === Botones === */
QPushButton {
    /* Fondo de botón neutro */
    background-color: #3e3e42; 
    /* Color de texto claro */
    color: #cccccc;
    /* Borde neutro */
    border: 1px solid #555555; 
    border-radius: 6px;
    padding: 8px 14px;
    font-weight: 400;
    font-size: 15px;
    letter-spacing: 0.3px;
}

QPushButton:hover {
    /* Color de acento azul para hover */
    background-color: #007acc; 
    color: #ffffff;
    border: 1px solid #007acc;
}

QPushButton:pressed {
    /* Color de acento azul oscuro para pressed */
    background-color: #005a9e; 
    color: #ffffff;
    border: 1px solid #005a9e;
}

/* Botón principal (Menú) */
#boton_menu {
    /* Color de acento para el botón principal */
    background-color: #007acc; 
    color: white;
    border: none;
    font-weight: 400;
}
#boton_menu:hover {
    background-color: #0063a5;
}
#boton_menu:pressed {
    background-color: #005a9e;
}

/* Botón salir */
#boton_salir {
    /* Color de peligro (rojo) */
    background-color: #cc4d4d; 
    color: white;
    border: none;
}
#boton_salir:hover {
    background-color: #a33b3b;
}
#boton_salir:pressed {
    background-color: #8a3232;
}

/* --- */

/* === Campos de texto === */
QLineEdit {
    /* Fondo del campo de texto */
    background-color: #252526; 
    /* Color de texto claro */
    color: #cccccc;
    /* Borde neutro */
    border: 1px solid #555555; 
    border-radius: 4px;
    padding: 6px 8px;
    font-size: 15px;
    font-weight: 300;
}

QLineEdit:focus {
    /* Borde de acento azul al enfocar */
    border: 1px solid #007acc; 
    /* Fondo de campo enfocado, ligeramente más claro */
    background-color: #2d2d30; 
}

/* --- */

/* === Labels === */
QLabel {
    /* Color de texto principal */
    color: #cccccc;
    font-weight: 300;
    font-size: 15px;
}

/* Label inferior o secundario */
#label {
    /* Color de texto secundario/apagado */
    color: #888888; 
    font-size: 13px;
    font-style: italic;
}

/* --- */

/* === Stacked Widget === */
QStackedWidget {
    /* Fondo del StackedWidget */
    background-color: #1e1e1e; 
    /* Borde sutil */
    border: 1px solid #444444; 
    border-radius: 6px;
}

/* --- */

/* === Scrollbars === */
QScrollBar:vertical {
    border: none;
    /* Fondo de la barra de desplazamiento */
    background: #2d2d30; 
    width: 10px;
    border-radius: 4px;
}
QScrollBar::handle:vertical {
    /* Color del 'handle' de la barra */
    background: #555555; 
    min-height: 20px;
    border-radius: 4px;
}
QScrollBar::handle:vertical:hover {
    /* Color de acento azul al pasar el ratón */
    background: #007acc; 
}
QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical {
    height: 0;
}

/* --- */

/* === Layouts === */
QVBoxLayout, QHBoxLayout {
    spacing: 10px;
}

/* --- */

/* === Botones laterales === */
#boton_agregar, #boton_buscar, #boton_actualizar, #boton_eliminar, #boton_consultar {
    /* Fondo de botón lateral */
    background-color: #252526; 
    /* Borde neutro */
    border: 1px solid #555555; 
    /* Color de texto claro */
    color: #cccccc;
    font-weight: 400;
    font-size: 15px;
}

#boton_agregar:hover, 
#boton_buscar:hover, 
#boton_actualizar:hover, 
#boton_eliminar:hover,
#boton_consultar:hover {
    /* Fondo de hover, más claro y con borde de acento */
    background-color: #333333; 
    border: 1px solid #007acc;
    /* Color de acento azul para el texto */
    color: #007acc; 
}

#boton_agregar:pressed, 
#boton_buscar:pressed, 
#boton_actualizar:pressed, 
#boton_eliminar:pressed ,
#boton_consultar:pressed{
    /* Fondo de pressed, más oscuro que el hover */
    background-color: #444444; 
}

/* --- */

/* === Tooltips === */
QToolTip {
    /* Fondo de tooltip */
    background-color: #252526; 
    /* Color de texto claro */
    color: #cccccc;
    /* Borde de acento */
    border: 1px solid #007acc; 
    padding: 6px;
    border-radius: 4px;
    font-size: 14px;
}</string>
