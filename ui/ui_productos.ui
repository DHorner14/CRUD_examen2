                <property name="styleSheet">
                 <string notr="true">/* === Tablas (Dark Mode) === */
QTableWidget {
    /* Fondo principal de la tabla */
    background-color: #252526; 
    /* Color de texto claro */
    color: #cccccc;
    /* Líneas de la cuadrícula sutiles */
    gridline-color: #3e3e42;
    /* Borde sutil */
    border: 1px solid #444444;
    border-radius: 6px;
    font-family: &quot;Segoe UI Variable&quot;, &quot;Roboto&quot;, &quot;Open Sans&quot;, sans-serif;
    font-size: 15px;
    font-weight: 300;
    /* Fondo de selección - Color de acento azul (similar al modo claro) */
    selection-background-color: #004d80; 
    /* Color del texto al seleccionar */
    selection-color: #ffffff;
    /* Filas alternadas, un gris ligeramente más oscuro que el fondo */
    alternate-background-color: #2d2d30; 
}

/* Cabecera horizontal */
QHeaderView::section {
    /* Fondo de la cabecera */
    background-color: #3e3e42;
    /* Color de texto claro */
    color: #cccccc;
    /* Borde sutil */
    border: 1px solid #555555;
    border-radius: 4px;
    font-weight: 400;
    font-size: 15px;
    padding: 8px;
}

/* Cabecera horizontal al pasar el cursor */
QHeaderView::section:hover {
    /* Fondo de hover de la cabecera, un poco más claro/acentuado */
    background-color: #505050;
}

/* Celdas */
QTableWidget::item {
    padding: 6px;
    border: none;
}

/* Celda seleccionada */
QTableWidget::item:selected {
    /* Mismo azul de selección que la tabla */
    background-color: #007acc; 
    /* Texto blanco para contraste */
    color: #ffffff;
}

/* Bordes del header alineados al contenido */
QHeaderView {
    border: none;
}

/* Barra de desplazamiento en la esquina (esquina de la tabla) */
QTableCornerButton::section {
    /* Fondo que coincide con la cabecera */
    background-color: #3e3e42;
    /* Borde sutil */
    border: 1px solid #555555;
}

/* Scrollbars coherentes dentro de tablas (ajustados a dark mode) */
QTableWidget QScrollBar:vertical {
    border: none;
    /* Fondo de la barra de desplazamiento */
    background: #2d2d30; 
    width: 10px;
    border-radius: 4px;
}
QTableWidget QScrollBar::handle:vertical {
    /* Color del 'handle' de la barra */
    background: #555555; 
    min-height: 20px;
    border-radius: 4px;
}
QTableWidget QScrollBar::handle:vertical:hover {
    /* Color de acento azul al pasar el ratón */
    background: #007acc; 
}
QTableWidget QScrollBar::add-line:vertical,
QTableWidget QScrollBar::sub-line:vertical {
    height: 0;
}</string>
